<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TextContainer">
        <attr name="title" format="string" />
        <attr name="placeholder" format="string" />
        <attr name="inputType" format="enum">
            <enum name="password" value="0" />
            <enum name="email" value="1" />
            <enum name="text" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ControlLayout">
        <attr name="errorMsg" format="string" />
    </declare-styleable>

</resources>